  "iconUrl": "http://serilog.net/images/serilog-sink-seq-nuget.png",
  "dependencies": {
    "Serilog": "2.0.0-beta-465",
    "Serilog.Sinks.PeriodicBatching": "2.0.0-beta-465",
    "Serilog.Sinks.RollingFile":  "2.0.0-beta-465"
  },
  "frameworks": {
